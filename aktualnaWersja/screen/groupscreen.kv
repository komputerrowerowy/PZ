                    color: 1, 1, 1, 1
            BoxLayout:
                orientation: 'vertical'
                size_hint: .5, 1
                Label:
                    id:label_max_speed2
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .3
                        Rectangle:
                            pos: self.pos
                            size: self.size

                    size_hint: 1, .7
                    font_size: self.height / 1.5
                    color: 1, 1, 1, 1
                Label:
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .3
                        Rectangle:
                            pos: self.pos
                            size: self.size
                    size_hint: 1, .2
                    font_size: self.height
                    text: "Maksymalna prędkość"
                    color: 1, 1, 1, 1
                Label:
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .3
                        Rectangle:
                            pos: self.pos
                            size: self.size
                    size_hint: 1, .1
                    font_size: self.height
                    text: ""
                    color: 1, 1, 1, 1

        BoxLayout:
            orientation: 'horizontal'
            size_hint: 1, 0.1
            valign: 'bottom'
