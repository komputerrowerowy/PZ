                    text: "Prędkość"
                    color: 1, 1, 1, 1
                Label:
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .3
                        Rectangle:
                            pos: self.pos
                            size: self.size
                    size_hint: 1, .1
                    font_size: self.height
